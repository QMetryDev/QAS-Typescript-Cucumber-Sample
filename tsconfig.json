{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"moduleResolution": "node",
		"sourceMap": false,
		"strict": false,
		"noUnusedLocals": false,
		"declaration": false,
		"removeComments": false,
		"noImplicitAny": false,
		"outDir": "out",
		"typeRoots": ["./node_modules/@types"],
		"types": ["node", "cucumber"]
	},
	"exclude": [
		"node_modules",
		"typescript"
	]
}